                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
                    xmlns:system="clr-namespace:System;assembly=System.Runtime">

    <FontFamily x:Key="FontAwesome">pack://application;,,,/Resources/Fonts/#FontAwesome</FontFamily>
    <FontFamily x:Key="SegoeMDL2">pack://application;,,,/Resources/Fonts/#Segoe MDL2 Assets</FontFamily>

    <system:String x:Key="SpinIconFontAwesome">&#xf110;</system:String>
    <system:String x:Key="CloseIconFontAwesome">&#xf00d;</system:String>

    <system:String x:Key="CloseIconSegoeMDL2">&#xE8BB;</system:String>
</ResourceDictionary>